<map proto="1.5.0" min-server-version="1.21.11"> <!-- Utilizes CMP fork features - End Crystals are not tracked in upstream PGM -->
<name>Radiant</name>
<version>1.0.1</version>
<created>2025-02-14</created>
<objective>Be on the team with the highest amount of kills after 3 minutes!</objective>
<gamemode>crystal</gamemode>
<gamemode>tdm</gamemode>
<include id="gapple-kill-reward"/>
<include id="void-death"/>
<time result="score">3m</time>
<constant id="void-plane">30</constant>
<terrain environment="the end"/>
<authors>
    <author uuid="9d4779a8-d35f-45d2-b51c-95de7a77e128"/> <!-- zzuf -->
</authors>
<teams>
    <team id="magenta-team" color="light purple" max="10">Magenta</team>
    <team id="yellow-team" color="yellow" max="10">Yellow</team>
</teams>
<score>
    <kills>1</kills>
</score>
<spawns>
    <spawn team="magenta-team" kit="spawn-kit">
        <regions>
            <point>1.5,102,-39.5</point>
        </regions>
    </spawn>
    <spawn team="yellow-team" kit="spawn-kit">
        <regions>
            <point yaw="-180">1.5,102,42.5</point>
        </regions>
    </spawn>
    <default>
        <regions>
            <point yaw="-90">-50.5,127,1.5</point>
            <point yaw="90">53.5,127,1.5</point>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="spawn-kit">
        <effect duration="1s" amplifier="255" ambient="true">damage resistance</effect>
        <item slot="slot.weapon.offhand" unbreakable="true" material="totem of undying"/>
        <item slot="0" amount="32" material="ender dragon spawn egg" components="item_name={'translate':'item.minecraft.end_crystal','extra':[{'text':' ('},{'translate':'options.framerateLimit.max','color':'aqua'},{'text':')','reset':'true'}]},item_model='minecraft:end_crystal',enchantment_glint_override=true,entity_data={id:'minecraft:end_crystal',ShowBottom:false}"/> 
        <item slot="1" unbreakable="true" material="netherite pickaxe" enchantment="efficiency:6"/>
        <item slot="2" amount="16" material="obsidian"/>
        <item slot="3" amount="8" material="ender pearl"/>
        <item slot="4" amount="8" material="golden apple"/>
        <item slot="5" unbreakable="true" material="crossbow"/>
        <item slot="6" material="scaffolding" amount="32"/>
<!--         <item slot="7" material="respawn anchor" amount="16"/> -->
        <item slot="7" name="`rFake Respawn Anchor" material="crying obsidian" amount="16"/>
        <item slot="8" material="glowstone" amount="64"/>
        <item slot="9" material="arrow" amount="16"/>
        <helmet material="netherite helmet" team-color="true" locked="true" unbreakable="true" enchantment="blast protection:4"/>
        <chestplate material="netherite chestplate" team-color="true" unbreakable="true" enchantment="projectile protection;blast protection:4"/>
        <leggings material="netherite leggings" team-color="true" unbreakable="true" enchantment="blast protection:4"/>
        <boots material="netherite boots" team-color="true" unbreakable="true" enchantment="blast protection:4"/>
    </kit>
</kits>
<filters>
    <blocks id="da-map" region="da-map-region">
        <any>
            <material>air</material>
        </any>
    </blocks>
</filters>
<regions>
    <cylinder id="da-map-region" base="1.5,53,1.5" height="38" radius="39.5"/>
    <apply block="da-map" message="You may not destroy the map!"/>
    <apply leave="never">
        <region>
            <below y="102"/>
        </region>
    </apply>
</regions>
<kill-rewards>
    <kill-reward>
        <item material="arrow" amount="4"/>
        <item material="ender dragon spawn egg" components="item_name={'translate':'item.minecraft.end_crystal','extra':[{'text':' ('},{'translate':'options.framerateLimit.max','color':'aqua'},{'text':')','reset':'true'}]},item_model='minecraft:end_crystal',enchantment_glint_override=true,entity_data={id:'minecraft:end_crystal',ShowBottom:false}" amount="8"/> 
        <item material="obsidian" amount="4"/>
        <item material="ender pearl" amount="4"/>
        <item material="totem of undying" amount="1"/>
    </kill-reward>
</kill-rewards>
<itemremove>
    <item>netherite helmet</item>
    <item>netherite chestplate</item>
    <item>netherite leggings</item>
    <item>netherite boots</item>
    <item>totem of undying</item>
    <item>glowstone dust</item>
    <item>netherite pickaxe</item>
    <item>end crystal</item>
    <item>ender dragon spawn egg</item>
    <item>iron fence</item>
    <item>obsidian</item>
    <item>crying obsidian</item>
    <item>ender pearl</item>
    <item>golden apple</item>
    <item>crossbow</item>
    <item>scaffolding</item>
    <item>respawn anchor</item>
    <item>glowstone</item>
    <item>arrow</item>
</itemremove>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
